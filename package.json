{
    "name": "motorway-takehome-backend",
    "version": "1.0.0",
    "description": "Install requirements:  - docker (https://docs.docker.com/get-docker/)",
    "main": "index.js",
    "scripts": {
        "start": "node ./dist/src/server.js",
        "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node'  src/server.ts",
        "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
        "lint:fix": "eslint --ignore-path .eslintignore --ext .ts,.js --fix .",
        "test": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests",
        "test-watch": "cross-env NODE_ENV=test jest -i --watch",
        "build": "rm -rf dist && tsc",
        "format": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}'",
        "format:check": "prettier --config .prettierrc.js --check './**/*.{js,ts,json,yml,yaml}'",
        "generate-client:node": "npm run generate-client:node-docker && sed -i '' 's+./dist+dist+g' clients/node/package.json",
        "prepare": "npx husky install"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/jest": "^29.2.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.18.2",
        "express-http-context": "^1.2.4",
        "helmet": "^4.6.0",
        "http-graceful-shutdown": "^3.1.3",
        "module-alias": "^2.2.2",
        "cross-env": "^7.0.2",
        "moment": "^2.29.4",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.19",
        "supertest": "^6.3.1",
        "typeorm": "^0.2.41",
        "typeorm-transactional-cls-hooked": "^0.1.21"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.11",
        "@types/module-alias": "^2.0.1",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.27.0",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^26.6.3",
        "jest-extended": "^0.11.5",
        "nodemon": "^2.0.20",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.8.1",
        "ts-node-dev": "^1.1.8",
        "tsc-watch": "^4.5.0",
        "typescript": "4.5.2",
        "winston": "^3.8.2"
    }
}
